{
  "name": "magic-mushroom",
  "version": "1.0.0",
  "description": "Starter kit for React Verse pwa",
  "scripts": {
    "setup": "node src/tools/setup/setupMessage.js && npm install && node src/tools/setup/setup.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prestart": "node_modules/.bin/babel-node src/tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src open:sw lint:watch",
    "open:src": "nodemon src/tools/srcServer.js --exec babel-node",
    "open:sw": "nodemon src/sw.js --exec babel-node",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel src -d dist/src",
    "lint": "node_modules/.bin/ esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "test": "node_modules/.bin/mocha --reporter progress src/tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "start-message": "babel-node src/tools/startMessage.js",
    "test:CI": "babel-node src/tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./src/tools/analyzeBundle.js"
  },
  "author": "Verse",
  "license": "MIT",
